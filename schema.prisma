generator client {
  provider = "prisma-client-js"
}

generator dto_generator {
  provider = "node node_modules/prisma-class-dto-generator"
  output   = "../src/prisma-models"
}
generator Test {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Profile {
  id  String @id
  age Int
}
